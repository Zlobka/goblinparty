---
import Layout from "../../../layouts/Layout.astro";
import Scroll from "../../../layouts/Scroll.astro";
import Scrollcard from "../../../layouts/Scrollcard.astro";
import Card from "../../../layouts/Card.astro";
---

<Layout title="Mons's Goblin Raiders">
	<Card
		img="/images/scryfall/creatures/mons-s-goblin-raiders-s99.jpg"
		enname="Mons's Goblin Raiders"
		runame="Гоблины-налетчики Монса"
		prints="Starter (1999)"
		illustrated="Pete Venters"
		link="https://scryfall.com/search?q=a%3A%E2%80%9CPete+Venters%E2%80%9D&order=released&dir=asc&unique=art"
		flavor="То, что у них есть клуб, не означает, что они организованы."
		thanks=""
		other="В июле 1999 года вышел набор карт начального уровня, известный как Starter 1999. В данном издании Гоблины-налетчики Монса были напечатаны с символом редкой карты, хотя она оставалась обычной. Подтип «Гоблины» изменен на единственное число, художественный текст поменяли на новый."
	/>
	<Scroll>
		<Scrollcard
			img="/images/scryfall/creatures/mons-s-goblin-raiders-lea.jpg"
			link="/Collection/Creatures/mons-s-goblin-raiders-lea"
		/>
		<Scrollcard
			img="/images/scryfall/creatures/mons-s-goblin-raiders-5ed.jpg"
			link="/Collection/Creatures/mons-s-goblin-raiders-5ed"
		/>
		<Scrollcard
			img="/images/scryfall/creatures/mons-s-goblin-raiders-s99.jpg"
			link="/Collection/Creatures/mons-s-goblin-raiders-s99"
		/>
		<Scrollcard
			img="/images/scryfall/creatures/mons-s-goblin-raiders-s00.jpg"
			link="/Collection/Creatures/mons-s-goblin-raiders-s00"
		/>
	</Scroll>
</Layout>

