---
import Layout from "../../../layouts/Layout.astro";
import Card from "../../../layouts/Card.astro";
import Scroll from "../../../layouts/Scroll.astro";
import Scrollcard from "../../../layouts/Scrollcard.astro";
---

<Layout title="Squee, Goblin Nabob">
	<Card
		img="/images/scryfall/creatures/squee-goblin-nabob-10e.jpg"
		enname="Squee, Goblin Nabob"
		runame="Скви, Гоблин-Набоб"
		prints="Tenth Edition (2007), Modern Masters (2013), Commander (2019), Commander Masters (2023)"
		illustrated="Greg Staples"
		link="https://scryfall.com/search?q=a%3A%E2%80%9CGreg+Staples%E2%80%9D&order=released&dir=asc&unique=art"
		flavor="«Некоторых гоблинов можно использовать и забыть. От некоторых, наоборот, невозможно избавиться. Этот же - и то, и другое, в одном лице!» \n — Старк"
		thanks=""
		other="В Десятом издании Скви переведен на русский язык, а тип карты переработан."
	/>
	<Scroll>
		<Scrollcard
			img="/images/scryfall/creatures/squee-goblin-nabob-mmq.jpg"
			link="/Collection/Creatures/squee-goblin-nabob-mmq"
		/>
		<Scrollcard
			img="/images/scryfall/creatures/squee-goblin-nabob-10e.jpg"
			link="/Collection/Creatures/squee-goblin-nabob-10e"
		/>
		<Scrollcard
			img="/images/scryfall/creatures/squee-goblin-nabob-uma.jpg"
			link="/Collection/Creatures/squee-goblin-nabob-uma"
		/>
		<Scrollcard
			img="/images/scryfall/creatures/squee-goblin-nabob-slc-1999.jpg"
			link="/Collection/Creatures/squee-goblin-nabob-slc-1999"
		/>
	</Scroll>
</Layout>
